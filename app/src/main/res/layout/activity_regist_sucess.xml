<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.qongyong.esq.Regist.Regist_sucess"
    android:background="#1772b4">

    <TextView
        android:id="@+id/regist_suce_sucess"
        android:layout_width="147dp"
        android:layout_height="36dp"
        android:layout_marginBottom="355dp"
        android:layout_marginEnd="102dp"
        android:layout_marginLeft="103dp"
        android:layout_marginRight="102dp"
        android:layout_marginStart="103dp"
        android:layout_marginTop="166dp"
        android:text="注册成功！"
        android:textColor="@android:color/background_light"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/regist_suce_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="57dp"
        android:layout_marginLeft="57dp"
        android:layout_marginRight="57dp"
        android:layout_marginStart="57dp"
        android:layout_marginTop="54dp"
        android:text="12345678910"
        android:textColor="@android:color/background_light"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/regist_suce_sucess" />

    <Button
        android:id="@+id/regist_suce_login"
        android:layout_width="206dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="139dp"
        android:layout_marginEnd="77dp"
        android:layout_marginLeft="77dp"
        android:layout_marginRight="77dp"
        android:layout_marginStart="77dp"
        android:layout_marginTop="70dp"
        android:background="@android:color/background_light"
        android:text="登录"
        android:textColor="@android:color/holo_blue_light"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/regist_suce_num"
        app:layout_constraintVertical_bias="0.0" />
</android.support.constraint.ConstraintLayout>